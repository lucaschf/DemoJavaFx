<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="334.0" prefWidth="349.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controller.MainWindowController">
    <children>
        <Button layoutX="100.0" layoutY="270.0" mnemonicParsing="false" onAction="#retrieveAndShowAction"
                prefHeight="34.0" prefWidth="150.0" text="Obter dados" AnchorPane.bottomAnchor="30.0"
                AnchorPane.rightAnchor="99.0"/>
        <TextField fx:id="nameTextField" layoutX="90.0" layoutY="66.0" prefHeight="25.0" prefWidth="235.0"
                   AnchorPane.leftAnchor="90.0" AnchorPane.rightAnchor="16.0" AnchorPane.topAnchor="66.0"/>
        <Label layoutX="46.0" layoutY="70.0" text="Nome" AnchorPane.leftAnchor="46.0" AnchorPane.topAnchor="70.0"/>
        <RadioButton fx:id="maleRadio" layoutX="187.0" layoutY="144.0" mnemonicParsing="false" selected="true"
                     text="Masculino">
            <toggleGroup>
                <ToggleGroup fx:id="gender"/>
            </toggleGroup>
        </RadioButton>
        <CheckBox fx:id="termsAgreementCheckBox" layoutX="88.0" layoutY="225.0" mnemonicParsing="false"
                  text="Li e Aceito os termos e condicoes" AnchorPane.leftAnchor="88.0"/>
        <ChoiceBox fx:id="civilStatusChoiceBox" layoutX="88.0" layoutY="174.0" prefWidth="150.0"
                   AnchorPane.leftAnchor="88.0"/>
        <Slider fx:id="ageSlider" layoutX="90.0" layoutY="109.0" max="120.0" value="18.0" AnchorPane.leftAnchor="90.0"/>
        <RadioButton fx:id="femaleRadio" layoutX="88.0" layoutY="144.0" mnemonicParsing="false" text="Feminino"
                     toggleGroup="$gender" AnchorPane.leftAnchor="88.0"/>
        <Label layoutX="47.0" layoutY="108.0" text="Idade" AnchorPane.leftAnchor="47.0" AnchorPane.topAnchor="108.0"/>
        <Label layoutX="47.0" layoutY="144.0" text="Sexo" AnchorPane.leftAnchor="47.0"/>
        <Label layoutX="14.0" layoutY="178.0" text="Estado civil" AnchorPane.leftAnchor="14.0"/>
        <Label alignment="CENTER" contentDisplay="RIGHT" layoutX="123.0" layoutY="14.0" text="Obtenção de atributos"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="16.0">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
